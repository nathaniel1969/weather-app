{
  "homepage": "https://nathaniel1969.github.io/weather-app",
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "description": "Weather app to display current, hourly and future forecast of cities",
  "keywords": [
    "node.js",
    "javascript",
    "react",
    "weather"
  ],
  "license": "ISC",
  "author": "Nathaniel Robinson",
  "main": "index.js",
  "dependencies": {
    "axios": "latest",
    "bootstrap": "latest",
    "chart.js": "^4.4.8",
    "chartjs-plugin-datalabels": "^2.2.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "predeploy": "npm run clean-cache && npm run build",
    "deploy": "gh-pages -d build",
    "start": "cross-env HOST=0.0.0.0 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-cache": "rimraf node_modules/.cache"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^6.3.0",
    "rimraf": "^6.0.1"
  }
}
